    "ajv": "^8.12.0",
    "chalk-template": "^1.1.0",
    "cosmiconfig": "^8.3.6",
    "http-proxy": "^1.18.1",
    "reflect-metadata": "^0.1.13",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@jujulego/jill": "2.4.0-alpha.3",
    "@jujulego/vite-plugin-swc": "1.1.0",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@rollup/plugin-json": "6.0.1",
    "@swc/cli": "0.1.62",
