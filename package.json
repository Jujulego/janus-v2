    "nodemon": "3.1.4",
    "rollup": "4.21.3",
    "shx": "0.3.4",
    "supertest": "7.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.6.2",
    "typescript-eslint": "8.5.0",
    "vite": "5.4.4",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.0"
  },
  "packageManager": "yarn@4.4.1",
  "resolutions": {
    "@graphql-eslint/eslint-plugin/@graphql-tools/graphql-tag-pluck": "^8.3.0",
    "@graphql-tools/code-file-loader/@graphql-tools/graphql-tag-pluck": "^8.3.0"
  }
