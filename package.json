    "@kyrielle/injector": "^1.0.0-alpha.2",
    "@kyrielle/logger": "^1.0.0-beta.1",
    "@reduxjs/toolkit": "^2.0.1",
    "@swc/helpers": "^0.5.11",
    "ajv": "^8.12.0",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "cosmiconfig": "^9.0.0",
    "graphql": "^16.8.1",
    "graphql-sse": "^2.5.2",
    "graphql-yoga": "^5.0.0",
    "http-errors": "^2.0.0",
    "http-proxy": "^1.18.1",
    "ink": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "is-unicode-supported": "^2.0.0",
    "kyrielle": "^1.0.0-beta.1",
    "log-symbols": "^7.0.0",
    "react": "^18.2.0",
    "reflect-metadata": "^0.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@codecov/rollup-plugin": "0.0.1-beta.12",
    "@eslint/compat": "1.1.1",
    "@eslint/js": "9.10.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.3.3",
    "@graphql-codegen/typescript": "4.0.9",
