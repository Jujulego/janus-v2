{
  "name": "@jujulego/janus",
  "version": "2.0.0-alpha.0",
  "license": "MIT",
  "author": "Julien Capellari <julien.capellari@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujulego/janus"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "./bin",
    "./dist"
  ],
  "type": "module",
  "bin": "./bin/janus.js",
  "scripts": {
    "lint": "eslint .",
    "clean": "shx rm -r dist",
    "build": "jill group 'build:esm // build:types'",
    "build:esm": "swc -d dist src",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "vitest run",
    "test:types": "vitest typecheck"
  },
  "dependencies": {
    "@jujulego/injector": "^0.0.5",
    "reflect-metadata": "^0.1.13",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@jujulego/jill": "2.3.3",
    "@jujulego/vite-plugin-swc": "1.1.0",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.95",
    "@types/node": "18.18.7",
    "@types/yargs": "17.0.29",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.52.0",
    "eslint-plugin-vitest": "0.3.8",
    "shx": "0.3.4",
    "typescript": "5.2.2",
    "vite": "5.0.0-beta.13",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "packageManager": "yarn@4.0.0"
}
