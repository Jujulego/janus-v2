    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.52.0",
    "eslint-plugin-vitest": "0.3.8",
    "rollup": "4.2.0",
    "shx": "0.3.4",
    "typescript": "5.2.2",
    "vite": "5.0.0-beta.15",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "packageManager": "yarn@4.0.1"
