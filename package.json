{
  "name": "@jujulego/janus",
  "version": "2.0.0-beta.5",
  "license": "MIT",
  "author": "Julien Capellari <julien.capellari@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujulego/janus-v2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "./bin",
    "./dist"
  ],
  "type": "module",
  "bin": "./bin/janus.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "shx rm -r dist",
    "codegen": "graphql-codegen-esm --config graphql.config.yml",
    "build": "jill run 'build:bundle // build:dts // build:types'",
    "build:bundle": "rollup -c",
    "build:dts": "tsc -p tsconfig.dts.json",
    "build:types": "tsc -p tsconfig.build.json",
    "watch": "rollup -c --watch",
    "dev:start": "nodemon ./bin/janus.js -- start -v",
    "test": "vitest run --typecheck"
  },
  "dependencies": {
    "@graphql-tools/merge": "^9.0.3",
    "@jujulego/pid-file": "^2.2.1",
    "@jujulego/quick-tag": "^1.0.1",
